BRILCALC

********************************                                                   ********************************
* online                                                                           * offline
********************************                                                   ********************************

brilcalc lumi \                                                                    brilcalc lumi \
--byls \                                                                           --byls \
--datatag online \                                                                 --normtag /afs/cern.ch/user/l/lumipro/public/normtag_file/normtag_DATACERT.json \
-i json.txt \                                                                      -i json.txt \
-u /ub \                                                                           -u /ub \
| tail -n +5 \                                                                     | tail -n +5 \
| head -n -9 \                                                                     | head -n -9 \
| awk -F "|" '{print $2","$3","$7","$8","$9}' \                                    | awk -F "|" '{print $2","$3","$7","$8","$9}' \
| awk -F ":" '{print $1","$2","$3}' \                                              | awk -F ":" '{print $1","$2","$3}' \
| awk '{ gsub (" ", "", $0); print}' \                                             | awk '{ gsub (" ", "", $0); print}' \
| awk -F "," '{print $1","$2","$4","$5","$6","$7}'                                 | awk -F "," '{print $1","$2","$4","$5","$6","$7}'
